    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-prettier": "3.1.0",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "rimraf": "3.0.0"
  }
}
